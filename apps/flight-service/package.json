    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/sequelize": "^4.28.20",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
